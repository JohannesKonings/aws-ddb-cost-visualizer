{
  "name": "app",
  "productName": "ddb-coviz",
  "version": "0.0.1",
  "description": "AWS DynamoDB Cost Visualizer",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-linux": "electron-forge make --platform linux --targets @electron-forge/maker-deb",
    "make-windows": "electron-forge make --platform win32 --targets @electron-forge/maker-squirrel",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Johannes Konings",
    "email": "johannes.konings@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/async-ora": "6.0.0-beta.63",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-wix": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "electron": "16.0.8",
    "eslint": "8.0.1",
    "eslint-plugin-import": "2.25.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0"
  }
}
